{
  "name": "NaverMap",
  "version": "0.1.0",
  "summary": "A CocoaPods wrapper for naver/navermap-ios-api.",
  "description": "A CocoaPods wrapper for navermap-ios-api.\nIt's not an offical CocoaPods.",
  "homepage": "https://github.com/wanbok/NaverMap",
  "license": "MIT",
  "authors": {
    "Wanbok Choi": "choi@wanbok.com"
  },
  "source": {
    "git": "https://github.com/wanbok/NaverMap.git",
    "tag": "0.1.0",
    "submodules": true
  },
  "social_media_url": "https://fb.me/wanbok",
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "resource_bundles": {
    "NaverMap": [
      "Pod/SDK/Resources/*.png"
    ]
  },
  "public_header_files": [
    "Pod/Module/**/*",
    "Pod/SDK/Lib/**/Headers/*.h"
  ],
  "vendored_libraries": [
    "Pod/SDK/Lib/ApiGateway/libApiGateway-MAC.a",
    "Pod/SDK/Lib/NMapViewer/libNMapViewer.a"
  ],
  "frameworks": [
    "UIKit",
    "CoreGraphics",
    "QuartzCore",
    "CoreLocation",
    "SystemConfiguration"
  ],
  "libraries": [
    "sqlite3",
    "xml2"
  ]
}
